   "source": [
    "def distinct(A):\n",
    "    s = set([abs(i) for i in A])\n",
    "    return len(s)\n",
    "if __name__ == '__main__':\n",
    " \n",
    "    input = [-1, -1, 0, 1, 1, 1]\n",
    " \n",
    "    print(\"The number of distinct absolute values are\", distinct(input))\n",
    " "
   ]
